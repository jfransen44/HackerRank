import java.io.*;
import java.util.*;
import java.math.BigInteger;
public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */      BigInteger bg = new BigInteger("1");
		Scanner s = new Scanner(System.in);
        int x = s.nextInt();
		while (x > 1){
			bg = bg.multiply(BigInteger.valueOf(x));
			x--;
		}
		System.out.println(bg);       
    }
}
